#: ../src/main.cc:322
msgid "OPTIONS:"
msgstr "OPTIONEN:"

#: ../src/main.cc:324
msgid "\t-h          show this usage message"
msgstr "\t-h          zeigt diese Verwendungsbeschreibung"

#: ../src/main.cc:325
msgid "\t-v          show version information"
msgstr "\t-v          zeigt Versionsinformationen"

